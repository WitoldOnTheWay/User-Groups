@model UserGroupsProject.Models.GetUserNamesViewModel

@{
    ViewBag.Title = "GetUserNames";
}



<table class="table">
    <tr>
        <th>
            @Html.DisplayFor(Model => Model.Group.Name)
        </th>
</tr>

    @foreach (var item in Model.User)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
                </td>
            <td>
               @Html.ActionLink("Back to list", "GetAll", "Group")
            </td>
        </tr>
    }
    @Html.ActionLink("Back to list", "GetAll", "Group")
</table>

